- model_class = BraintreeRails::Transaction
.page-header
  %h1="New #{model_class.model_name.human}"
= render :partial => "form"

- content_for :javascript do
  :javascript
    var braintree = Braintree.create("#{ENV["CLIENT_SIDE_ENCRYPTION_KEY"]}")
    braintree.onSubmitEncryptForm('braintree-transaction-form');
